<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hu.cewi.client.user">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".CEWiApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.register.RegisterUserActivity" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".ui.devices.DeviceActivity" />
        <activity android:name=".ui.devicecontent.DeviceContentActivity"></activity>
    </application>

</manifest>